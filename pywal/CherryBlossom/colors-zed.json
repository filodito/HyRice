{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#110b0790",
                "editor.background": "#110b0790",
                "editor.foreground": "#c3c2c1",
                "text": "#c3c2c1",
                "text.muted": "#c3c2c170",
                "text.ignored": "#c3c2c140",
                "text.placeholder": "#c3c2c150",
                "ignored": "#c3c2c130",
                "element.hover": "#c3c2c130",
                "ghost_element.hover": "#B6A48A30",
                "ghost_element.selected": "#B6A48A30",
                "ghost_element.active": "#B6A48A60",
                "border": "#110b07",
                "editor.highlighted_line.background": "#B6A48A10",
                "editor.active_line.background": "#B6A48A10",
                "panel.background": "#110b0790",
                "title_bar.background": "#110b0790",
                "title_bar.inactive_background": "#110b0790",
                "status_bar.background": "#110b0790",
                "drop_target.background": "#110b0790",
                "elevated_surface.background": "#110b07",
                "toolbar.background": "#110b0790",
                "tab_bar.background": "#110b0790",
                "tab.inactive_background": "#110b0790",
                "tab.active_background": "#B6A48A30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#110b07",
                "scrollbar.thumb.background": "#B6A48A",
                "editor.gutter.background": "#110b0790",
                "terminal.background": "#110b0710",
                "terminal.foreground": "#c3c2c1",
                "terminal.dim_foreground": "#c3c2c1",
                "terminal.bright_foreground": "#c3c2c1",
                "terminal.ansi.black": "#110b07",
                "terminal.ansi.red": "#B6A48A",
                "terminal.ansi.green": "#D4B495",
                "terminal.ansi.yellow": "#D8CDAD",
                "terminal.ansi.blue": "#E0CFAC",
                "terminal.ansi.magenta": "#E7D5B6",
                "terminal.ansi.cyan": "#E9DDC3",
                "terminal.ansi.white": "#c3c2c1",
                "terminal.ansi.bright_black": "#6a5f56",
                "terminal.ansi.bright_red": "#B6A48A",
                "terminal.ansi.bright_green": "#D4B495",
                "terminal.ansi.bright_yellow": "#D8CDAD",
                "terminal.ansi.bright_blue": "#E0CFAC",
                "terminal.ansi.bright_magenta": "#E7D5B6",
                "terminal.ansi.bright_cyan": "#E9DDC3",
                "terminal.ansi.bright_white": "#c3c2c1",
                "modified": "#B6A48A",
                "syntax": {
                    "attribute": {
                        "color": "#B6A48A"
                    },
                    "boolean": {
                      "color": "#D4B495"
                    },
                    "comment": {
                      "color": "#6a5f56"
                    },
                    "comment.doc": {
                      "color": "#6a5f56"
                    },
                    "constant": {
                      "color": "#D4B495"
                    },
                    "function": {
                      "color": "#E9DDC3"
                    },
                    "keyword": {
                      "color": "#E0CFAC"
                    },
                    "number": {
                      "color": "#E7D5B6"
                    },
                    "operator": {
                      "color": "#E0CFAC"
                    },
                    "string": {
                      "color": "#B6A48A"
                    },
                    "variable": {
                      "color": "#D4B495"
                    }
                },
                "players": [
                  {
                    "cursor": "#B6A48A",
                    "background": "#110b07",
                    "selection": "#B6A48A30"
                  }
                ]
            }
        }
    ]
}
